<!DOCTYPE html>
<html lang="en" class="no-js">
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title></title>

    <title>home</title>

    <meta name="description" content="A sidebar menu as seen on the Google Nexus 7 website" />
    <meta name="keywords" content="google nexus 7 menu, css transitions, sidebar, side menu, slide out menu" />
    <meta name="author" content="Codrops" />

    <link rel="shortcut icon" href="../favicon.ico">
    <link rel="stylesheet" type="text/css" href="/css/normalize.css" />
    <link rel="stylesheet" type="text/css" href="/css/demo.css" />
    <link rel="stylesheet" type="text/css" href="/css/component.css" />
    <script src="/js/modernizr.custom.js"></script>


    <link href="https://fonts.googleapis.com/css?family=Rubik:300,400,500" rel="stylesheet">
    <link href="https://unpkg.com/ionicons@4.4.4/dist/css/ionicons.min.css" rel="stylesheet">

    <link rel="stylesheet" href="/css/bootstrap.css">
    <link rel="stylesheet" href="/css/animate.css">
    <link rel="stylesheet" href="/css/owl.carousel.min.css">

    <link rel="stylesheet" href="/fonts/ionicons/css/ionicons.min.css">
    <link rel="stylesheet" href="/fonts/fontawesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="/fonts/flaticon/font/flaticon.css">
    <link rel="stylesheet" href="/css/magnific-popup.css">

    <!-- Theme Style -->
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
<div >




    <ul id="gn-menu" class="gn-menu-main" style="z-index:1;">
        <li class="gn-trigger">
            <a class="gn-icon gn-icon-menu"><span>Menu</span></a>
            <nav class="gn-menu-wrapper">
                <div class="gn-scroller">
                    <ul class="gn-menu">
                        <li><a href="/user/" class="gn-icon ion-ios-person">Home</a></li>

                        <li><a href="/user/profile" class="gn-icon ion-ios-person">Profile</a></li>
                        <li><a href="/edit_profile" class="gn-icon ion-ios-person">Edit Profile</a></li>
                        <li><a href="/user/my_articles" class="gn-icon gn-icon-article">My Articles</a></li>

                        <li><a href="/user/new_article" class="gn-icon ion-md-add-circle-outline">Create Article</a></li>
                      <!--  <li><a class="gn-icon  ion-ios-settings">Settings</a></li> -->

                        <li><a href="/user/logout" class="gn-icon ion-md-log-out">Logout</a></li>
                       <!-- <li><a class="gn-icon ion-md-log-out">Logout</a></li>
                        <li><a class="gn-icon ion-md-log-out">Logout</a></li>

                        <li><a class="gn-icon gn-icon-archive">Archives</a></li> -->



                        </li>
                    </ul>
                </div><!-- /gn-scroller -->
            </nav>
        </li>

        <li class="navbar-nav absolute-right">
            <a href="#">logout</a>
        </li>

    </ul>
</div>




<div class="abc" style="margin-top: 60px">





    <!-- END section -->

    <section class="site-section bg-light element-animate" id="section-counter">
        <div class="container">
            <div class="row">
                <div class="col-lg-6">
                    <div class="text-center">
                        <img src="/uploadnew/<%= student.Index %>.png" class="img-responcive img-circle img-thumbnail" alt="No image uploaded" width="250px" height="250px">

                    </div><br>




                    <div class="row">
                        <div class="col-lg-6 col-sm-12">
                            <ul class="list-group">
                                <% if(student) { %>
                                    <li class="list-group-item text-muted"><%= student.firstname %></li>

                                    <li class="list-group-item text-right"><span class="pull-left"><%= student.lastname %></li>
                                    <li class="list-group-item text-right"><span class="pull-left"> <%= student.mobile %></li>

                            </ul>
                            <% } %>
                        </div>

                        <div class="col-lg-6 col-sm-12">
                            <ul class="list-group">
                                <% if(student) { %>
                                    <li class="list-group-item text-muted"><%= student.email %></li>

                                    <li class="list-group-item text-right"><span class="pull-left"><%= student.address %></li>
                                    <li class="list-group-item text-right"><span class="pull-left"><%= student.course %></li>

                            </ul>
                            <% } %>
                        </div>
                    </div>



                </div>

                <div class="col-lg-5 ml-auto">
                    <div class="block-15">
                        <div class="heading">
                            <h2>Some Numbers</h2>
                        </div>

                    </div>

                    <div class="row">
                        <div class="col-md-6">

                            <div class="block-18 d-flex align-items-center">
                                <div class="icon mr-4">
                                    <span class="flaticon-books"></span>
                                </div>
                                <div class="text">
                                    <strong class="number" ><%= articles%></strong>
                                    <span>Articles</span>
                                </div>
                            </div>


                            <div class="block-18 d-flex align-items-center">
                                <div class="icon mr-4">
                                    <span class="flaticon-student"></span>
                                </div>
                                <div class="text">
                                    <strong class="number" ><%= following%></strong>
                                    <span>Following</span>
                                </div>
                            </div>

                        </div>
                        <div class="col-md-6">
                            <div class="block-18 d-flex align-items-center">
                                <div class="icon mr-4">
                                    <span class="flaticon-student"></span>
                                </div>
                                <div class="text">
                                    <strong class="number" ><%= followers%></strong>
                                    <span>Followers</span>
                                </div>
                            </div>

                            <div class="block-18 d-flex align-items-center">
                                <div class="icon mr-4">
                                    <span class="flaticon-mortarboard"></span>
                                </div>
                                <div class="text">
                                    <strong class="number" >0</strong>
                                    <span>Likes</span>
                                </div>
                            </div>

                        </div>
                    </div>

                </div>
            </div>
        </div>
    </section>
    <!-- END section -->



    <!-- end section -->
    <!-- loader -->
    <div id="loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee"/><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#f4b214"/></svg></div>

    <script src="/js/jquery-3.2.1.min.js"></script>
    <script src="/js/jquery-migrate-3.0.0.js"></script>
    <script src="/js/popper.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/owl.carousel.min.js"></script>
    <script src="/js/jquery.waypoints.min.js"></script>
    <script src="/js/jquery.stellar.min.js"></script>
    <script src="/js/jquery.animateNumber.min.js"></script>

    <script src="/js/jquery.magnific-popup.min.js"></script>

    <script src="/js/main.js"></script>


</div><!-- /abc -->

<script src="/js/classie.js"></script>
<script src="/js/gnmenu.js"></script>
<script>
    new gnMenu( document.getElementById( 'gn-menu' ) );
</script>
</body>
</html>