<!DOCTYPE html>
<html lang="en" class="no-js">
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Articles</title>
    <meta name="description" content="A sidebar menu as seen on the Google Nexus 7 website" />
    <meta name="keywords" content="google nexus 7 menu, css transitions, sidebar, side menu, slide out menu" />
    <meta name="author" content="Codrops" />

    <link rel="shortcut icon" href="../favicon.ico">
    <link rel="stylesheet" type="text/css" href="/css/normalize.css" />
    <link rel="stylesheet" type="text/css" href="/css/demo.css" />
    <link rel="stylesheet" type="text/css" href="/css/component.css" />
    <script src="/js/modernizr.custom.js"></script>


    <link href="https://fonts.googleapis.com/css?family=Rubik:300,400,500" rel="stylesheet">
    <link href="https://unpkg.com/ionicons@4.4.4/dist/css/ionicons.min.css" rel="stylesheet">

    <link rel="stylesheet" href="/css/bootstrap.css">
    <link rel="stylesheet" href="/css/animate.css">
    <link rel="stylesheet" href="/css/owl.carousel.min.css">

    <link rel="stylesheet" href="/fonts/ionicons/css/ionicons.min.css">
    <link rel="stylesheet" href="/fonts/fontawesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="/fonts/flaticon/font/flaticon.css">
    <link rel="stylesheet" href="/css/magnific-popup.css">
    <style>
        html{height: 100%;}
        body {
            min-height: 100vh;
        }
    </style>
    <!-- Theme Style -->
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
<div class="container">
    <ul id="gn-menu" class="gn-menu-main" style="z-index:1;">
        <li class="gn-trigger">
            <a class="gn-icon gn-icon-menu"><span>Menu</span></a>
            <nav class="gn-menu-wrapper">
                <div class="gn-scroller">
                    <ul class="gn-menu">
                        <li><a href="/user/profile" class="gn-icon ion-ios-person">Profile</a></li>
                        <li><a href="/edit_profile" class="gn-icon ion-ios-person">Edit Profile</a></li>
                        <li><a href="/user/my_articles" class="gn-icon gn-icon-article">My Articles</a></li>

                        <li><a href="/user/new_article" class="gn-icon ion-md-add-circle-outline">Create Article</a></li>
                        <!--  <li><a class="gn-icon  ion-ios-settings">Settings</a></li> -->

                        <li><a href="/user/logout" class="gn-icon ion-md-log-out">Logout</a></li>



                        </li>
                    </ul>
                </div><!-- /gn-scroller -->
            </nav>
        </li>

        <li class="absolute-right" style="background-color: lightcyan ;">
            <div style="height: fit-content" >
                <form action="/user/search" method="post" class="d-block d-lg-flex">
                    <div class="fields d-block d-lg-flex">
                        &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
                        <div class="textfield-search  one-third"><input style="
                                                                            line-height: 44px;" type="text" name="keywords" class="form-control" placeholder="Keyword search..."></div>
                        &nbsp;&nbsp;&nbsp;

                        <div class="textfield-search one-third"><input  style="
                                                                            line-height: 44px;" type="text" class="form-control" name="author" placeholder="Author search..."></div>

                    </div>
                    &nbsp;
                    <input type="submit" class="search-submit btn btn-primary" style="
                                                                            height: 58px;"value="Search">
                </form>

                <div id="result" class="u" style="">

                </div>
            </div>
        </li>
    </ul>
</div>
<div class="abc" style="margin-top: 60px;">
    <div class="site-section bg-light">
        <div class="container">
            <div class="row justify-content-center mb-5 element-animate">
                <h2>Your Articles</h2>
            </div>
            <div class="row element-animate">

                <% for(var i=0; i< articles.length; i++) { %>

                <div class="col-md-2 col-lg-1">
                </div>

                    <div class="col-md-10 col-lg-5">

                        <div class="block-21 d-flex mb-4">
                            <figure class="mr-3">
                                <a href="#"><img src="/images/img_3.jpg" alt="" class="img-fluid"></a>
                            </figure>
                            <div class="text">
                                <h3 class="heading"><a href="/user/my_articles/<%= articles[i]._id %>"><%= articles[i].title%></a></h3>
                                <div class="meta">
                                    <div><a href="#"><span class="ion-android-calendar"></span> <%= articles[i].date.toDateString() %></a></div>

                                    <div><a href="#"><span class="ion-chatbubble"></span> 19</a></div>
                                </div>
                            </div>
                        </div>
                    </div>

                <% } %>








            </div>
        </div>
    </div>


    <!-- END footer -->

    <!-- loader -->
    <div id="loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee"/><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#f4b214"/></svg></div>

    <script src="/js/jquery-3.2.1.min.js"></script>
    <script src="/js/jquery-migrate-3.0.0.js"></script>
    <script src="/js/popper.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/owl.carousel.min.js"></script>
    <script src="/js/jquery.waypoints.min.js"></script>
    <script src="/js/jquery.stellar.min.js"></script>
    <script src="/js/jquery.animateNumber.min.js"></script>

    <script src="/js/jquery.magnific-popup.min.js"></script>

    <script src="/js/main.js"></script>
</div><!-- /container -->
</div>
<script src="/js/classie.js"></script>
<script src="/js/gnmenu.js"></script>
<script>
    new gnMenu( document.getElementById( 'gn-menu' ) );
</script>
</body>
</html>